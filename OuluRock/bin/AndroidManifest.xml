<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tundrarunning.oulurock"
    android:versionCode="5"
    android:versionName="1.0.3" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name="com.tundrarunning.oulurock.ActivityName"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tundrarunning.oulurock.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.tundrarunning.oulurock.ActivityName" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tundrarunning.oulurock.ActivityName" />
        </activity>
    </application>

</manifest>
